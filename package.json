{
  "name": "vue-ele-form-generator",
  "version": "3.1.0",
  "description": "vue-ele-form-generator是专为vue-ele-form开发的可视化表单设计工具, 让表单开发的效率更上一层楼",
  "private": false,
  "license": "MIT",
  "scripts": {
    "bootstrap": "yarn || npm i",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --mode=lib --target lib --name vue-ele-form-generator --dest ./lib/ ./src/main.js",
    "lint": "vue-cli-service lint src/*.{vue,js} --mode production",
    "release": "./release.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dream2023/vue-ele-form-generator"
  },
  "dependencies": {},
  "keywords": [
    "vue-ele-form",
    "vue-ele-form-generator",
    "form-schema",
    "vue-form",
    "form-designer",
    "form-design",
    "element-ui",
    "form-generator",
    "for-generate",
    "vue-form-generator",
    "form schema"
  ],
  "homepage": "https://vue-ele-form-generator.netlify.com/",
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.6",
    "@vue/cli-plugin-eslint": "~4.4.6",
    "@vue/cli-plugin-vuex": "~4.4.6",
    "@vue/cli-service": "~4.4.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "clipboard-copy": "^3.1.0",
    "commitizen": "^4.0.3",
    "core-js": "^3.6.5",
    "cz-conventional-changelog": "^3.0.2",
    "ejs": "^3.1.3",
    "element-ui": "^2.13.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "github-release-notes": "^0.17.1",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.19",
    "normalize.css": "^8.0.1",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "serialize-javascript": "^3.1.0",
    "standard-version": "^7.0.1",
    "vue": "^2.6.11",
    "vue-ele-form": "^0.8.25",
    "vue-ele-form-bmap": "^0.1.0",
    "vue-ele-form-codemirror": "^0.1.1",
    "vue-ele-form-dynamic": "^0.4.0",
    "vue-ele-form-gallery": "^0.1.1",
    "vue-ele-form-image-uploader": "^0.1.8",
    "vue-ele-form-json-editor": "^0.1.3",
    "vue-ele-form-markdown-editor": "^0.1.2",
    "vue-ele-form-quill-editor": "^0.1.4",
    "vue-ele-form-table-editor": "^0.1.3",
    "vue-ele-form-tree-select": "^0.1.1",
    "vue-ele-form-upload-file": "^0.0.6",
    "vue-ele-form-video-uploader": "^0.1.4",
    "vue-multipane": "^0.9.5",
    "vue-template-compiler": "^2.6.11",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.3",
    "vuex-persistedstate": "^3.0.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
