{
  "name": "f-render",
  "version": "4.1.0",
  "private": false,
  "description": "f-render | 基于 vue-ele-form 的表单设计器",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "toc": "doctoc README.md",
    "build:lib": "vue-cli-service build --target lib --name f-render --dest ./dist/ ./src/index.js",
    "lint": "vue-cli-service lint --mode=production",
    "pub": "npm run build:lib && npm publish && git push && git push gitee"
  },
  "main": "./dist/f-render.umd.min.js",
  "dependencies": {
    "clipboard-copy": "^3.1.0",
    "fast-deep-equal": "^3.1.3",
    "fuzzy": "^0.1.3",
    "indent-string": "^4.0.0",
    "lodash": "^4.17.20",
    "prismjs": "^1.21.0",
    "serialize-javascript": "^4.0.0",
    "vue-ele-form-data-editor": "^0.1.3",
    "vue-ele-form-dynamic": "^0.4.0",
    "vue-multipane": "^0.9.5",
    "vue-prism-editor": "^1.2.2",
    "vue2-perfect-scrollbar": "^1.5.0",
    "vuedraggable": "^2.24.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "core-js": "^3.6.5",
    "doctoc": "^1.4.0",
    "element-ui": "^2.13.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "lodash-webpack-plugin": "^0.11.5",
    "normalize.css": "^8.0.1",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-ele-form": "^0.8.34",
    "vue-template-compiler": "^2.6.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dream2023/vue-ele-form-generator"
  },
  "keywords": [
    "vue-ele-form",
    "vue-ele-form-generator",
    "form-schema",
    "vue-form",
    "form-designer",
    "form-design",
    "element-ui",
    "form-generator",
    "for-generate",
    "vue-form-generator",
    "form schema"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint --mode=production",
      "git add"
    ]
  }
}
