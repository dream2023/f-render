  "homepage": "https://vue-ele-form-generator.netlify.com/",
  "dependencies": {},
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/lodash-es": "^4.17.3",
    "@types/lz-string": "^1.3.33",
